{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "esModuleInterop": true,
    "declaration": false,
    "module": "ES2020",
    "target": "ES2020",
    "lib": [
      "ES2020",
      "dom"
    ],
    "moduleResolution": "bundler",
    "experimentalDecorators": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "strict": true,
    "strictPropertyInitialization": false,
    "useDefineForClassFields": false
  }
}
