{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "query": {
      "root": "",
      "sourceRoot": "query",
      "projectType": "library",
      "prefix": "gc",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "options": {
            "tsConfig": "./tsconfig.lib.json",
            "project": "./ng-package.json"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
